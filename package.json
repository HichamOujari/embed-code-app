{
  "name": "embed-code-app",
  "version": "1.0.0",
  "description": "embed-code-app by Komoroho",
  "main": "main.js",
  "build": {
    "appId": "com.electron.1221",
    "productName": "embed-code-app",
    "asar": false, 
    "mac": {
      "target": [
        "pkg"
      ],
      "entitlements": "entitlements.mac.plist"
    },
    "win": {
      "target": [
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "msi",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  },
  "scripts": {
    "start": "electron .",
    "make": "electron-forge make",
    "dist": "electron-builder"
  },
  "keywords": [
    "komoroho",
    "embed-code-app"
  ],
  "author": "Komoroho",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.50",
    "@electron-forge/maker-deb": "^6.0.0-beta.50",
    "@electron-forge/maker-rpm": "^6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.50",
    "@electron-forge/maker-zip": "^6.0.0-beta.50",
    "devtron": "^1.4.0",
    "electron": "^17.1.2",
    "electron-builder": "^22.14.13"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "electron-squirrel-startup": "^1.0.0"
  }
}
